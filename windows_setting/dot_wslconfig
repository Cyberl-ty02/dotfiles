[wsl2]
# 方便配置代理
networkingMode=VirtioProxy
# 启用 DNS 隧道（可选，但通常推荐）
dnsTunneling=true
# 启用 Windows 防火墙规则
firewall=true
# 启用自动代理（Windows中设为系统代理）
autoProxy=true
